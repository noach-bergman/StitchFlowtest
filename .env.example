# Gemini (required for AI features)
API_KEY=your_gemini_api_key
# Backward-compat alias (optional)
# GEMINI_API_KEY=your_gemini_api_key

# Supabase (optional: if missing, app runs in local mode)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Vite-style aliases (optional)
# VITE_API_KEY=your_gemini_api_key
# VITE_GEMINI_API_KEY=your_gemini_api_key
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Server-side print queue (required for Zebra cloud printing)
# Use service role key only on trusted server runtime (API/worker), never expose to browser clients.
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
PRINT_API_SHARED_SECRET=replace_with_strong_random_secret
PRINT_WORKER_POLL_INTERVAL_MS=2000
PRINT_SOCKET_TIMEOUT_MS=7000
PRINT_FAILED_ALERT_THRESHOLD=10
PRINT_FAILED_ALERT_WINDOW_MINUTES=10

# Frontend print queue config
# Leave VITE_PRINT_API_BASE_URL empty to call same-origin /api routes.
# VITE_PRINT_API_BASE_URL=https://your-app-domain.com
VITE_PRINT_SOURCE=web
VITE_PRINT_DEFAULT_PRINTER_ID=default-zebra
# SECURITY NOTE: this key is embedded into frontend build if set.
# Prefer calling a trusted backend that signs requests instead of exposing this key in public web apps.
VITE_PRINT_API_SHARED_SECRET=replace_with_same_shared_secret_if_needed
# Optional status polling tuning
# VITE_PRINT_STATUS_POLL_MS=2000
# VITE_PRINT_STATUS_MAX_ATTEMPTS=30

# Optional fallback admin credentials
# DEFAULT_ADMIN_USERNAME=admin
# DEFAULT_ADMIN_PASSWORD=change_me
