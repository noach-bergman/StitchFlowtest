# Gemini (required for AI features)
API_KEY=your_gemini_api_key
# Backward-compat alias (optional)
# GEMINI_API_KEY=your_gemini_api_key

# Supabase (optional: if missing, app runs in local mode)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key

# Vite-style aliases (optional)
# VITE_API_KEY=your_gemini_api_key
# VITE_GEMINI_API_KEY=your_gemini_api_key
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# QZ Tray bridge for one-click USB label printing (Mac/Windows)
# Set to true to use QZ first, then fallback to browser print on failure.
# VITE_QZ_USE_BRIDGE=true
# Exact printer name as shown in your OS printer list (recommended).
# VITE_QZ_PRINTER_NAME=NIIMBOT B1
# Optional custom websocket endpoint. Default QZ localhost probing is used if omitted.
# VITE_QZ_TRAY_WS_URL=ws://localhost:8182
# Optional signed mode (recommended) to avoid repeated QZ permission prompts.
# Run local signer at http://127.0.0.1:9123 first.
# VITE_QZ_CERT_URL=http://127.0.0.1:9123/qz/cert
# VITE_QZ_SIGN_URL=http://127.0.0.1:9123/qz/sign
# VITE_QZ_SIGN_ALGORITHM=SHA512

# Unified print provider (auto|ble|qz|agent|browser)
# auto = BLE first, then QZ, then browser fallback
# VITE_PRINT_PROVIDER=auto
# BLE Agent endpoint/token (required for provider=ble or auto)
# VITE_BLE_AGENT_URL=http://127.0.0.1:9131
# VITE_BLE_AGENT_TOKEN=replace-with-strong-token
# Local NIIMBOT App Agent endpoint/token (legacy fallback, optional)
# VITE_LOCAL_AGENT_URL=http://127.0.0.1:9130
# VITE_LOCAL_AGENT_TOKEN=replace-with-strong-token

# Optional fallback admin credentials
# DEFAULT_ADMIN_USERNAME=admin
# DEFAULT_ADMIN_PASSWORD=change_me
